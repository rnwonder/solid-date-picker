    "solidjs-picker",
    "solid-range-picker",
    "solid-date-picker",
    "multi-date-picker",
    "solidjs-multi-date-picker",
    "solidjs-multi-date",
    "mulitple-date-picker",
    "solidjs-multiple-date-picker",
    "solidjs time picker",
    "solidjs timepicker",
    "solidjs time"
  ],
  "author": "rnwonder",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "types": "tsc",
    "build": "pnpm run clean && pnpm run clean-read-me && vite build --mode lib && cp -R ../style.css ./dist/ && cp -R ../README.md ./",
    "serve": "vite preview",
    "test": "vitest --ui",
    "clean": "IF EXIST \"types\" ( rmdir /s /q \"types\" )",
    "clean-read-me": "IF EXIST \"README.md\" ( del /f /q \"README.md\" )"
  },
  "bugs": "https://github.com/rnwonder/solid-date-picker/issues",
  "homepage": "https://soliddatepicker.netlify.app",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.11.16",
    "@vitest/ui": "^1.2.2",
    "autoprefixer": "^10.4.17",
    "clsx": "^1.2.1",
    "daisyui": "^4.6.2",
    "del": "^7.1.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "postcss": "^8.4.35",
    "postcss-cli": "^10.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "sass": "^1.70.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^4.9.5",
    "vite": "^5.0.12",
